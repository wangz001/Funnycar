@{
    ViewBag.Title = "搜索";
}

<div class="page">
    <header class="bar bar-nav">
        <a class="icon icon-left pull-left open-panel" href="/Travels"></a>
        <span class="title">@ViewBag.Title</span>
    </header>
    <div class="content">
        <div class="searchbar row">
            <div class="search-input col-85">
                <input type="search" id='search' placeholder='输入关键字...' ng-model="yourname"/>
            </div>
            <a class="button button-fill button-primary col-15"><span class="icon icon-search"></span></a>
        </div>
        <hr>
        Hello {{yourname || 'World'}}!
    </div>

</div>
